{
  "manifest_version": 2,
  "name": "BlockDistraction",
  "version": "1.0",
  "description": "Block the list of website after 5min of usage",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "blockscript.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
